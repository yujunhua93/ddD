<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NetView">
        <attr name="netColor" format="color"></attr>
        <attr name="overLayColor" format="color"></attr>
        <attr name="textColor" format="color"></attr>
        <attr name="overLayAlpha" format="integer"></attr>
        <attr name="tagSize" format="integer"></attr>
    </declare-styleable>

    <declare-styleable name="FruitLoadingView">
        <attr name="shadowColor" format="color"></attr>
        <attr name="maxHeight" format="dimension"></attr>
        <attr name="fruitHeight" format="dimension"></attr>
        <attr name="fruitWidth" format="dimension"></attr>
        <attr name="animationDuration" format="integer"></attr>
        <attr name="fruitDuration" format="integer"></attr>
        <attr name="fruitDrawable" format="reference"></attr>
    </declare-styleable>

    <declare-styleable name="ScratchView">
        <attr name="outter_bg" format="reference"></attr>

    </declare-styleable>
    <declare-styleable name="StyleImageView">
        <attr name="style">
            <enum name="none" value="-1"/>
            <enum name="saturation" value="0"/>
            <enum name="grey_scale" value="1"/>
            <enum name="invert" value="2"/>
            <enum name="rgb_to_bgr" value="3"/>
            <enum name="sepia" value="4"/>
            <enum name="black_and_white" value="5"/>
            <enum name="bright" value="6"/>
            <enum name="vintage_pinhole" value="7"/>
            <enum name="kodachrome" value="8"/>
            <enum name="technicolor" value="9"/>
        </attr>
        <attr name="brightness" format="integer"/>
        <attr name="contrast" format="float"/>
        <attr name="saturation" format="float"/>
        <attr name="enable_animation" format="boolean"/>
        <attr name="animation_duration" format="integer"/>
    </declare-styleable>

</resources>